{
  "name": "Contact",
  "description": "Object that descripes an entry in the contact list of a DecentSearch server.",
  "definition": {
    "type": "object",
    "attributes": [
      {
        "key": "url",
        "type": "URL",
        "required": true
      }, {
        "key": "identity",
        "type": "PEM",
        "required": true
      }, {
        "key": "hashRangeURL",
        "type": "HashRange",
        "required": true
      }, {
        "key": "hashRangeData",
        "type": "HashRange",
        "required": true
      }, {
        "key": "reports",
        "type": "array",
        "required": true,
        "min": 0, 
        "max": 100,
        "items": [{
          "type": "Report"
        }]
      }
    ] 
  }
}
